    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Kasper",
            "homepage": "http://jordankasper.com",
            "role": "Developer"
        },
        {
            "name": "Ben Ramsey",
            "homepage": "http://benramsey.com",
            "role": "Developer"
        },
        {
            "name": "Jacques Woodcock",
            "homepage": "http://jacqueswoodcock.tumblr.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.*",
